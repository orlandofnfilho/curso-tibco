<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:pfx2="http://www.tibco.com/schemas/2.1UsersApi/SharedResources/Schemas/StandardError.xsd2" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns1="http://www.tibco.com/pe/EngineTypes">
    <xsd:import namespace="http://www.tibco.com/schemas/2.1UsersApi/SharedResources/Schemas/StandardError.xsd2" schemaLocation="/SharedResources/Schemas/StandardError.xsd"/>
    <pd:name>Processes/DeleteUserImpl.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="id" type="xsd:string"/>
    </pd:startType>
    <pd:startX>93</pd:startX>
    <pd:startY>77</pd:startY>
    <pd:returnBindings>
        <xsl:choose>
            <xsl:when test="$DeletarUsuario/resultSet/outputSet/RETURN_CODE =1">
                <Response>
                    <statusCode>
                        <xsl:value-of select="200"/>
                    </statusCode>
                    <message>
                        <xsl:value-of select="$Render-JSON/ns:ActivityOutputClass/jsonString"/>
                    </message>
                </Response>
            </xsl:when>
            <xsl:otherwise>
                <Response>
                    <statusCode>
                        <xsl:value-of select="404"/>
                    </statusCode>
                    <message>
                        <xsl:value-of select="$RenderExceptionHandler/ns:ActivityOutputClass/jsonString"/>
                    </message>
                </Response>
            </xsl:otherwise>
        </xsl:choose>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="Response">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="statusCode" type="xsd:int"/>
                    <xsd:element name="message" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:endType>
    <pd:endX>523</pd:endX>
    <pd:endY>75</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1687250105374</pd:targetNamespace>
    <pd:activity name="Render JSON">
        <pd:type>com.tibco.plugin.json.activities.JSONRenderActivity</pd:type>
        <pd:resourceType>ae.activities.JSONRenderActivity</pd:resourceType>
        <pd:x>380</pd:x>
        <pd:y>78</pd:y>
        <config>
            <SchemaType>xsdType</SchemaType>
            <BadgerfishCheck>false</BadgerfishCheck>
            <NullforEmptyvalues>true</NullforEmptyvalues>
            <ValidateIntputEnable>true</ValidateIntputEnable>
            <ActivityInputEditor>
                <xsd:element name="message" type="xsd:string"/>
            </ActivityInputEditor>
        </config>
        <pd:inputBindings>
            <ns:ActivityInputClass>
                <message>
                    <xsl:value-of select="'User has been deleted'"/>
                </message>
            </ns:ActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="DeletarUsuario">
        <pd:type>com.tibco.plugin.jdbc.JDBCCallActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCCallActivity</pd:resourceType>
        <pd:x>234</pd:x>
        <pd:y>78</pd:y>
        <config>
            <timeout>10</timeout>
            <maxRows>100</maxRows>
            <emptyStrAsNil>true</emptyStrAsNil>
            <jdbcSharedConfig>/SharedResources/Connections/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <catalog>regescweb_db</catalog>
            <ProcedureName>DeletarUsuario</ProcedureName>
            <useSchema>true</useSchema>
            <oraObjects/>
            <oraTables/>
            <ConfigurationError>output schema is null</ConfigurationError>
            <parameterTypes>
                <parameter>
                    <colName>userId</colName>
                    <colType>1</colType>
                    <dataType>4</dataType>
                    <typeName>INT</typeName>
                </parameter>
                <parameter>
                    <colName>RETURN_CODE</colName>
                    <colType>4</colType>
                    <dataType>4</dataType>
                    <typeName>INT</typeName>
                </parameter>
            </parameterTypes>
        </config>
        <pd:inputBindings>
            <inputs>
                <inputSet>
                    <userId>
                        <xsl:value-of select="$Start/id"/>
                    </userId>
                </inputSet>
            </inputs>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Log">
        <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
        <pd:resourceType>ae.activities.log</pd:resourceType>
        <pd:x>523</pd:x>
        <pd:y>195</pd:y>
        <config>
            <role>Error</role>
        </config>
        <pd:inputBindings>
            <pfx:ActivityInput>
                <message>
                    <xsl:value-of select="$Catch/exceptiondata/*"/>
                </message>
                <xsl:if test="$_error/ns1:ErrorReport/MsgCode">
                    <msgCode>
                        <xsl:value-of select="$_error/ns1:ErrorReport/MsgCode"/>
                    </msgCode>
                </xsl:if>
            </pfx:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>644</pd:x>
        <pd:y>195</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="RenderExceptionHandler">
        <pd:type>com.tibco.plugin.json.activities.JSONRenderActivity</pd:type>
        <pd:resourceType>ae.activities.JSONRenderActivity</pd:resourceType>
        <pd:x>239</pd:x>
        <pd:y>218</pd:y>
        <config>
            <SchemaType>xsdType</SchemaType>
            <BadgerfishCheck>false</BadgerfishCheck>
            <NullforEmptyvalues>true</NullforEmptyvalues>
            <ValidateIntputEnable>false</ValidateIntputEnable>
            <removeRootElementForInput>true</removeRootElementForInput>
            <ActivityInputEditor ref="pfx2:StandardError"/>
        </config>
        <pd:inputBindings>
            <ns:ActivityInputClass>
                <pfx2:StandardError>
                    <pfx2:timestamp>
                        <xsl:value-of select="current-dateTime()"/>
                    </pfx2:timestamp>
                    <pfx2:status>
                        <xsl:value-of select="404"/>
                    </pfx2:status>
                    <pfx2:error>
                        <xsl:value-of select="'Resource not found'"/>
                    </pfx2:error>
                    <pfx2:message>
                        <xsl:value-of select="concat('User not found id: ', $Start/id)"/>
                    </pfx2:message>
                    <pfx2:path>
                        <xsl:value-of select="concat('/users?id=', $Start/id)"/>
                    </pfx2:path>
                </pfx2:StandardError>
            </ns:ActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Render JSON</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>DeletarUsuario</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>DeletarUsuario</pd:from>
        <pd:to>Render JSON</pd:to>
        <pd:xpathDescription>User found</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$DeletarUsuario/resultSet/outputSet/RETURN_CODE = 1</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Log</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>Log</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>DeletarUsuario</pd:from>
        <pd:to>RenderExceptionHandler</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>RenderExceptionHandler</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>